<h1>Addition of two numbers</h1>
<script>
    var a, b, sum;
    a = parseInt(prompt("Enter first number:"));
    b = parseInt(prompt("Enter second number:"));
    sum = a + b;
    alert("The sum of the numbers is: " + sum);
</script>
</body>
